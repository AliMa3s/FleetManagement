<Window x:Class="FleetManagement.WPF.ZoekWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FleetManagement.WPF"
        mc:Ignorable="d"
        Title="ZoekWindow" Height="600" Width="850">
    <Grid>
        <TabControl>
            <TabItem x:Name="VoertuigZoekTab" Header="Voertuigen" FontSize="14" PreviewMouseDown="VoertuigZoekTab_PreviewMouseDown"/>
            <TabItem x:Name="BestuurderZoekTab" Header="Bestuurders" FontSize="14" PreviewMouseDown="BestuurderZoekTab_PreviewMouseDown"></TabItem>
            <TabItem x:Name="TankkaartZoekTab" Header="Tankkaarten" FontSize="14" PreviewMouseDown="TankkaartZoekTab_PreviewMouseDown"></TabItem>
            <TabItem x:Name="AutomodelZoekTab" Header="Automodellen" FontSize="14" PreviewMouseDown="AutomodelZoekTab_PreviewMouseDown"></TabItem>
        </TabControl>
    </Grid>
</Window>
