<Window x:Class="FleetManagement.WPF.ZoekWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FleetManagement.WPF"
        mc:Ignorable="d"
        Title="ZoekWindow" Height="450" Width="800">
    <Grid>
        <TabControl Background="LightBlue">
            <TabItem Header="Overzicht" FontSize="14">
                <TextBlock x:Name="authUser"></TextBlock>
            </TabItem>
            <TabItem Header="Voertuigen" FontSize="14"></TabItem>
            <TabItem Header="Bestuurders" FontSize="14"></TabItem>
            <TabItem Header="Tankkaarten" FontSize="14"></TabItem>
            <TabItem Header="AutoModellen" FontSize="14"></TabItem>
            <TabItem Header="Brandstoffen" FontSize="14"></TabItem>
            <TabItem Header="Kleuren" FontSize="14"></TabItem>
        </TabControl>
    </Grid>
</Window>
